package com.lg.dao;

import java.util.List;

import com.lg.dto.FreeDTO;

public interface FreeDAO {
	public List<FreeDTO> boardList() throws Exception;

	public FreeDTO boardDetail(int seq) throws Exception;
	
	public void freeInsert(FreeDTO dto) throws Exception;
	
	public void freeDelete(int seq) throws Exception;
	
	public void freeEdit(FreeDTO dto) throws Exception;
}
